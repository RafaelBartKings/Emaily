{
	"builds": [
		{
			"src": "index.js",
			"use": "@vercel/node",
			"config": {
				"includeFiles": [
					"client/build/**"
				]
			}
		},
		{
			"src": "client/build/**",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/index.js"
		},
		{
			"src": "/static/(.*)",
			"dest": "/client/build/static/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/client/build/index.html"
		}
	]
}